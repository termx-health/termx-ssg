---
layout: default
---

<div style="display: flex; justify-content: space-between; align-items: center">
  <h1>{{ page.title }} </h1>

  <span>
  {% assign sorted = page.langs | sort_natural %}
    {% for lang in sorted %}
      {% if lang.key == page.language %}
        <span>{{ lang.key }}</span>
      {% else %}
        <a href="/{{ lang.key }}/{{ lang.value }}">{{ lang.key }}</a>
      {% endif %}
    {% endfor %}
  </span>
</div>

{{ content }}


